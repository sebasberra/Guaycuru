{
  "name": "property-information",
  "version": "3.2.0",
  "description": "Information for HTML properties",
  "license": "MIT",
  "keywords": [
    "html",
    "property",
    "attribute",
    "information",
    "info"
  ],
  "repository": {
    "type": "git",
