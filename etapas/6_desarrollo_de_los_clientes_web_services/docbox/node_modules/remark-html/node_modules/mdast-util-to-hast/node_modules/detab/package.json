{
  "name": "detab",
  "version": "2.0.1",
  "description": "Detab: tabs -> spaces",
  "license": "MIT",
  "keywords": [
    "detab",
    "tab",
    "space",
    "tab-size",
    "size"
  ],
  "repository": {
    "type": "git",
