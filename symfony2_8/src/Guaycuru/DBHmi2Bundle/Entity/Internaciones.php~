    /**
     * @var string
     *
     * @ORM\Column(name="dom_cod_dpto", type="string", length=3, nullable=false)
     */
    private $domCodDpto;

    /**
     * @var string
     *
     * @ORM\Column(name="dom_cod_prov", type="string", length=2, nullable=false)
     */
    private $domCodProv;

    /**
     * @var string
     *
     * @ORM\Column(name="dom_cod_pais", type="string", length=3, nullable=false)
     */
    private $domCodPais;

