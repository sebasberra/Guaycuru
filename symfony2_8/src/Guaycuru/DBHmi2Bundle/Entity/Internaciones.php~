     *
     * @ORM\Column(name="ing_edad", type="boolean", nullable=false)
     */
    private $ingEdad;

    /**
     * @var string
     *
     * @ORM\Column(name="ing_tipo_edad", type="string", length=1, nullable=false)
     */
