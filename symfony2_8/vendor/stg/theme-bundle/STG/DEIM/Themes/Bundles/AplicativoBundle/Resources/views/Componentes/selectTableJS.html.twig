{% block jquery %}
<!-- BLOQUE JQUERY -->
<script>
        $(function() {

            $("#selectable").selectable({
                stop: function() {
                    $( "#select-result" ).val('');
                    $( ".ui-selected", this ).each(function() {
                        var index = $( this ).data( 'idelement' );
                        $( "#select-result" ).val($( "#select-result" ).val() + "#" + ( index ));
                    });
                }
            });
        });
    </script>
{% endblock %}
