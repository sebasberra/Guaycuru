<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Efectores" table="efectores">
    <indexes>
      <index name="idx_fk_efectores_id_localidad" columns="id_localidad"/>
      <index name="idx_fk_efectores_id_dependencia_adm" columns="id_dependencia_adm"/>
      <index name="idx_fk_efectores_id_regimen_juridico" columns="id_regimen_juridico"/>
      <index name="idx_fk_efectores_id_nodo" columns="id_nodo"/>
      <index name="idx_fk_efectores_id_subnodo" columns="id_subnodo"/>
      <index name="idx_fk_efectores_id_nivel_complejidad" columns="id_nivel_complejidad"/>
      <index name="idx_clavesisa" columns="clavesisa"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_unique_claveestd" columns="claveestd"/>
    </unique-constraints>
    <id name="idEfector" type="integer" column="id_efector">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idNodo" type="smallint" column="id_nodo" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="idSubnodo" type="smallint" column="id_subnodo" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="idDependenciaAdm" type="boolean" column="id_dependencia_adm" nullable="true"/>
    <field name="idRegimenJuridico" type="boolean" column="id_regimen_juridico" nullable="false"/>
    <field name="idNivelComplejidad" type="boolean" column="id_nivel_complejidad" nullable="false">
      <options>
        <option name="comment">Nivel de complejidad del ejector. Ver tabla niveles_complejidades</option>
      </options>
    </field>
    <field name="claveestd" type="string" column="claveestd" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clavesisa" type="string" column="clavesisa" length="14" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="comment">clave SISA (Sistema Integrado de Informacion Argentino)</option>
      </options>
    </field>
    <field name="tipoEfector" type="string" column="tipo_efector" length="1" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="comment">tipo establecimiento para el informe de estadistica</option>
      </options>
    </field>
    <field name="nomEfector" type="string" column="nom_efector" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomRedEfector" type="string" column="nom_red_efector" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nodo" type="boolean" column="nodo" nullable="false"/>
    <field name="subnodo" type="smallint" column="subnodo" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="internacion" type="boolean" column="internacion" nullable="false">
      <options>
        <option name="comment">Indica si el efector tiene internacion o no</option>
      </options>
    </field>
    <field name="baja" type="boolean" column="baja" nullable="false">
      <options>
        <option name="comment">Indica si el efector esta activo o fue dado de baja</option>
      </options>
    </field>
    <many-to-one field="idLocalidad" target-entity="Localidades" fetch="LAZY">
      <join-columns>
        <join-column name="id_localidad" referenced-column-name="id_localidad"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
