<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ServiciosEstadistica" table="servicios_estadistica">
    <indexes>
      <index name="idx_fk_servicios_estadistica_id_servicio" columns="id_servicio"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="idx_unique_cod_servicio_sector_subsector" columns="cod_servicio,sector,subsector"/>
    </unique-constraints>
    <id name="idServicioEstadistica" type="integer" column="id_servicio_estadistica">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idServicio" type="integer" column="id_servicio" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">id de tabla servicios</option>
      </options>
    </field>
    <field name="codServicio" type="string" column="cod_servicio" length="3" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="comment">codigo nuclear de servicios de nacion vigente desde 2008</option>
      </options>
    </field>
    <field name="sector" type="string" column="sector" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="comment">1=varones; 2=mujeres; 3=mixto</option>
      </options>
    </field>
    <field name="subsector" type="string" column="subsector" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="comment">4=internacion; 5=CE; 6=atencion domiciliaria;7=?internacion;8=?internacion</option>
      </options>
    </field>
    <field name="nomServicioEstadistica" type="string" column="nom_servicio_estadistica" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">descripcion del servicio obtenida del SIPES, tabla de servicios de 5 digitos</option>
      </options>
    </field>
    <field name="nomRedServicioEstadistica" type="string" column="nom_red_servicio_estadistica" length="30" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">idem anterios truncada con los primeros 30 caracteres</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
